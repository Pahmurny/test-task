  "type": "object",
  "properties": {
    "id": {
      "$id": "/properties/id",
      "type": "integer",
      "title": "The Id Schema ",
      "default": 0,
      "examples": [
        12345678
      ]
    },
    "created": {
      "$id": "/properties/created",
      "type": "string",
      "title": "The Created Schema ",
      "default": "",
